{
  "name": "dev-tools",
  "description": "Developer tools for the TxMA project",
  "author": "GDS",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.163.0",
    "@aws-sdk/client-s3": "3.163.0",
    "axios": "0.27.2"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@types/aws-lambda": "8.10.102",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "aws-sdk-client-mock": "2.0.0",
    "aws-sdk-client-mock-jest": "2.0.0",
    "esbuild": "0.15.12",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.3",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-extended": "3.1.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml-cfn": "0.3.1"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
